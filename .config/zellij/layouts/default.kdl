                command_host_format     "{stdout}"
                command_host_interval   "0"
                command_host_rendermode "static"

                command_user_command    "whoami"
                command_user_format     "{stdout}"
                command_user_interval   "10"
                command_user_rendermode "static"

                datetime          "{format}"
                // datetime_format   "%Y-%m-%d Û∞Öê %H:%M"
                datetime_format   "üïë %H:%M"
                datetime_timezone "Asia/Kolkata"

                format_left   "#[bg=#313244,fg=#eba0ac]ÓÇ∂#[bg=#eba0ac,fg=#181926,bold]ÓØà {session} {mode}#[bg=#282828] {tabs}"
                format_center "{notifications}"
                // format_right  "#[fg=#89dceb]ÓÇ∂#[fg=#11111b,bg=#89dceb]Ôë≥ #[fg=#89dceb,bg=#313244] {command_user}@{command_host}#[fg=#313244]ÓÇ¥#[fg=#74c7ec]ÓÇ∂#[fg=#11111b,bg=#74c7ec]Û∞É≠ #[fg=#74c7ec,bg=#313244] {datetime}#[fg=#313244]ÓÇ¥ "
                format_right "#[fg=#89dceb]ÓÇ∂#[fg=#11111b,bg=#89dceb]Ôë≥ #[fg=#89dceb,bg=#313244] {command_user}@{command_host}#[fg=#313244]ÓÇ¥#[fg=#74c7ec]ÓÇ∂#[fg=#11111b,bg=#74c7ec]üìÖ #[fg=#74c7ec,bg=#313244] {datetime}#[fg=#313244]ÓÇ¥ "
